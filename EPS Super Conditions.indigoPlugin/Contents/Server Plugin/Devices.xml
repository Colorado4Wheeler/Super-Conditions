		<ConfigUI>
			<Field type="checkbox" id="allowSave" defaultValue="false" hidden="true">
				<Label> </Label>
				<Description>When we need to pop a warning of some kind</Description>
			</Field>
			
			<Field type="list" id="devicelist" defaultValue="" rows="6">
				<Label>Devices:</Label>
				<List class="self" filter="onOffState" method="getDevicesWithStates" dynamicReload="true"/>
			</Field>
			
			<Field id="timeOut" type="textfield" defaultValue="5">
				<Label>Auto off minutes:</Label>
			</Field>
			
			<Field type="checkbox" id="physicalOnly" defaultValue="true">
				<Label> </Label>
				<Description>Only when physically switched on</Description>
			</Field>
			
			<Field type="checkbox" id="tapDuration" defaultValue="true" >
				<Label> </Label>
				<Description>Extend duration when switched on repeatedly</Description>
			</Field>
			
			<Field id="extendTime" type="textfield" defaultValue="5" visibleBindingId="tapDuration" visibleBindingValue="true">
				<Label>Minutes to extend by:</Label>
			</Field>
			
			<Field id="turnOn" type="textfield" defaultValue="3">
				<Label>Repeats before staying on:</Label>
			</Field>
			
			
